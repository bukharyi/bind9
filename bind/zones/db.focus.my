;
; BIND data file for local loopback interface
;
$TTL	1200
@	IN	SOA	ns1.focus.my. admin.focus.my. (
			      3		; Serial
			   1800		; Refresh (30 minutes)
			    900		; Retry   (15 minutes)
			1209600 	; Expire  (2 weeks)
			 1200 )		; Negative Cache TTL (20 minutes)
; name servers - ns records
	IN	NS	ns1.focus.my.

; name servers - A records
ns1		IN	A	192.168.138.3



; 192.168.138.0/24 - A records
ns1		IN	A	192.68.138.3
