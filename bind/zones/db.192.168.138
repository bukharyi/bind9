;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns1.focus.my.  admin.focus.my. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; name servers - NS records
	IN	NS	ns1.focus.my.

; PTR Records
3	IN	PTR	ns1.focus.my.
